{
	"coc.source.file.enable": true,
	"coc.source.around.enable": true,
	"coc.source.buffer.enable": true,
	"coc.source.dictionary.enable": true,
	"coc.source.tag.enable": true,
	"coc.source.omni.enable": true,
	"coc.source.omni.filetypes": ["txt"],

	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.hintSign": "ღ",
	"diagnostic.infoSign": "ʘ",
	"diagnostic.errorSign": "",
	"diagnostic.warningSign": "",
	"diagnostic.enable": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",

	"list.source.gfiles.defaultOptions": ["--auto-preview"],
	"list.source.outline.defaultOptions": ["--auto-preview"],

	"git.enableGutters": true,
	"git.branchCharacter":"\uf408",
	"git.addGBlameToBufferVar": true,
	"git.addGBlameToVirtualText": false,
	"git.virtualTextPrefix": " ❯❯❯ ",
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",

	"coc.preferences.formatOnSaveFiletypes": ["css"],
	"prettier.statusItemText": "★ ",
	"prettier.eslintIntegration": true,
	"prettier.tslintIntegration": true,
	"prettier.stylelintIntegration": false,

	"suggest.completionItemKindLabels": {
		"function": "",
		"constructor": "",
		"method": "",
		"variable": "",
		"constant": "",
		"struct": "",
		"class": "",
		"interface": "",
		"text": "",
		"enum": "",
		"enumMember": "",
		"module": "",
		"color": "b",
		"property": "",
		"field": "",
		"unit": "",
		"file": "",
		"value": "",
		"event": "",
		"folder": "",
		"keyword": "",
		"snippet": "",
		"operator": "",
		"reference": "",
		"typeParameter": "",
		"default": ""
	},

	"tsserver.enable": true,
	"tsserver.disableAutomaticTypeAcquisition": true,
}
